{
  "name": "Dataline.ExtraStandard.GkvKomServer",
  "version": "1.0.0-*",
  "title": "eXTra-Basis-Definitionen (GKV-Kommunikationsserver)",
  "authors": [ "DATALINE GmbH & Co. KG" ],
  "owners": [ "DATALINE GmbH & Co. KG" ],
  "summary": "eXTra-Basis-Definitionen (GKV-Kommunikationsserver)",
  "description": "eXTra-Basis-Definitionen für das GKV-Kommunikationsserver-Verfahren",
  "licenseUrl": "https://github.com/dataline-gmbh/ExtraStandard/blob/master/LICENSE.md",
  "copyright": "Copyright © 2015-2016 DATALINE GmbH & Co. KG",
  "tags": [ "eXTra", "standard", "basis", "definition", "GKV", "komserver", "kommunikationsserver", "verfahren" ],
  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "compilationOptions": {
        "debugType": "none",
        "xmlDoc": true
      }
    }
  },
  "compilationOptions": {
    "keyFile": "../../Dataline.ExtraStandard.snk"
  },
  "packInclude": {
    "/": "../../*.md"
  },
  "compile": [ "../../GlobalAssemblyInfo.cs" ],
  "dependencies": {
    "Dataline.ExtraStandard": "*"
  },
  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.0.0-*"
      }
    },
    "net40": {
      "compilationOptions": {
        "define": [ "HAS_FULL_TYPE" ]
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile259": {
      "compilationOptions": {
        "define": [ "PCL" ]
      },
      "frameworkAssemblies": {
        "System.Runtime": { "type": "build" }
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile336": {
      "compilationOptions": {
        "define": [ "HAS_FULL_TYPE", "PCL" ]
      },
      "frameworkAssemblies": {
        "mscorlib":  { "type": "build" }
      }
    }
  }
