{
  "name": "Dataline.ExtraStandard.GkvKomServer.Extra13",
  "version": "1.0.0-*",
  "title": "eXTra-Standard 1.3 (GKV-Kommunikationsserver)",
  "authors": [ "DATALINE GmbH & Co. KG" ],
  "description": "Definitionen für den eXTra-Standard 1.3 (GKV-Kommunikationsserver)",
  "copyright": "Copyright © 2015-2016 DATALINE GmbH & Co. KG",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "buildOptions": {
        "debugType": "none",
        "xmlDoc": true,
        "optimize": true
      }
    }
  },
  "buildOptions": {
    "keyFile": "../../Dataline.ExtraStandard.snk",
    "compile": [ "../../GlobalAssemblyInfo.cs" ],
    "embed": {
      "include": "Schemas/**/*.xsd"
    }
  },
  "packOptions": {
    "summary": "Definitionen für den eXTra-Standard 1.3 (GKV-Kommunikationsserver)",
    "owners": [ "DATALINE GmbH & Co. KG" ],
    "licenseUrl": "https://github.com/dataline-gmbh/ExtraStandard/blob/master/LICENSE.md",
    "tags": [ "eXTra", "standard", "1.3", "definition", "GKV", "komserver", "kommunikationsserver", "verfahren" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/dataline-gmbh/ExtraStandard"
    },
    "files": {
      "mappings": {
        "/": "../../*.md"
      }
    }
  },
  "dependencies": {
    "Dataline.ExtraStandard": { "target": "project" }
  },
  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    },
    "net40": {
      "buildOptions": {
        "define": [ "HAS_FULL_TYPE" ]
      },
      "frameworkAssemblies": {
      }
    },
    "net45": {
      "buildOptions": {
        "define": [ "HAS_FULL_TYPE" ]
      },
      "frameworkAssemblies": {
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile259": {
      "buildOptions": {
        "define": [ "PCL" ]
      },
      "frameworkAssemblies": {
        "System": { "type": "build" },
        "System.Runtime": { "type": "build" }
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile336": {
      "buildOptions": {
        "define": [ "HAS_FULL_TYPE", "PCL" ]
      },
      "frameworkAssemblies": {
        "mscorlib": { "type": "build" },
        "System": { "type": "build" }
      }
    }
  }
}
