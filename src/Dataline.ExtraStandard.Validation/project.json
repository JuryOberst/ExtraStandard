{
  "name": "Dataline.ExtraStandard.Validation",
  "version": "1.0.0-*",
  "title": "Basis-Definitionen für eXTra-Dokument-Validierungen",
  "authors": [ "DATALINE GmbH & Co. KG" ],
  "description": "Basis-Definitionen für die Validierung von eXTra-Dokumenten",
  "copyright": "Copyright © 2015-2016 DATALINE GmbH & Co. KG",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "buildOptions": {
        "debugType": "none",
        "xmlDoc": true,
        "optimize": true
      }
    }
  },
  "buildOptions": {
    "keyFile": "../../Dataline.ExtraStandard.snk",
    "compile": [ "../../GlobalAssemblyInfo.cs" ]
  },
  "packOptions": {
    "summary": "Basis-Definitionen für die Validierung von eXTra-Dokumenten",
    "owners": [ "DATALINE GmbH & Co. KG" ],
    "licenseUrl": "https://github.com/dataline-gmbh/ExtraStandard/blob/master/LICENSE.md",
    "tags": [ "eXTra", "standard", "basis", "validierung", "validation" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/dataline-gmbh/ExtraStandard"
    },
    "files": {
      "mappings": {
        "/": "../../*.md"
      }
    }
  },
  "dependencies": {
    "Dataline.ExtraStandard": { "target": "project" }
  },
  "frameworks": {
    "net40": {
      "buildOptions": {
        "define": [ "HAS_FULL_TYPE" ]
      },
      "frameworkAssemblies": {
        "System.Xml": "*",
        "System.Xml.Linq": "*"
      }
    },
    "net45": {
      "buildOptions": {
        "define": [ "HAS_FULL_TYPE" ]
      },
      "frameworkAssemblies": {
        "System.Xml": "*",
        "System.Xml.Linq": "*"
      }
    }
  }
}
