{
  "name": "Dataline.ExtraStandard",
  "version": "1.0.0-*",
  "title": "eXTra-Standard-Basisdefinitionen",
  "authors": [ "DATALINE GmbH & Co. KG" ],
  "description": "Basis-Definitionen für den eXTra-Standard",
  "copyright": "Copyright © 2015-2016 DATALINE GmbH & Co. KG",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "buildOptions": {
        "debugType": "none",
        "xmlDoc": true
      }
    }
  },
  "buildOptions": {
    "keyFile": "../../Dataline.ExtraStandard.snk",
    "compile": [ "../../GlobalAssemblyInfo.cs" ]
  },
  "packOptions": {
    "summary": "Basis-Definitionen für den eXTra-Standard",
    "owners": [ "DATALINE GmbH & Co. KG" ],
    "licenseUrl": "https://github.com/dataline-gmbh/ExtraStandard/blob/master/LICENSE.md",
    "tags": [ "eXTra", "standard", "basis" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/dataline-gmbh/ExtraStandard"
    },
    "files": {
      "mappings": {
        "/": "../../*.md"
      }
    }
  },
  "dependencies": { },
  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.0.0-*",
        "System.Xml.XmlSerializer": {
          "version" : "4.0.11-*"
        }
      }
    },
    "net40": {
      "buildOptions": {
        "define": [ "HAS_FULL_TYPE" ]
      },
      "frameworkAssemblies": {
        "System.Xml": "4.0.0",
        "System.Xml.Linq": "4.0.0"
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile259": {
      "buildOptions": {
        "define": [ "PCL" ]
      },
      "frameworkAssemblies": {
        "System":  { "type": "build" },
        "System.Runtime": { "type": "build" },
        "System.Diagnostics.Tools": { "type": "build" },
        "System.Xml.XmlSerializer": { "type": "build" },
        "System.Linq": { "type": "build" },
        "System.Text.Encoding": { "type": "build" },
        "System.Xml.Linq": { "type": "build" },
        "System.Collections": { "type": "build" },
        "System.Xml.XDocument": { "type": "build" },
        "System.Reflection": { "type": "build" },
        "System.Threading": { "type": "build" },
        "System.IO": { "type": "build" },
        "System.Xml.ReaderWriter": { "type": "build" }
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile336": {
      "buildOptions": {
        "define": [ "HAS_FULL_TYPE", "PCL" ]
      },
      "frameworkAssemblies": {
        "mscorlib": { "type": "build" },
        "System": { "type": "build" },
        "System.Core": { "type": "build" },
        "System.Xml": { "type": "build" },
        "System.Xml.Linq": { "type": "build" },
        "System.Xml.Serialization": { "type": "build" },
      }
    }
  }
}
