{
  "name": "Dataline.ExtraStandard.Encryption",
  "version": "1.0.0-*",
  "title": "eXTra-Standard-Krypto-Verfahren",
  "authors": [ "DATALINE GmbH & Co. KG" ],
  "description": "Unterstützte Krypto-Verfahren für den eXTra-Standard",
  "copyright": "Copyright © 2015-2016 DATALINE GmbH & Co. KG",
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "buildOptions": {
        "debugType": "none",
        "xmlDoc": true
      }
    }
  },

  "buildOptions": {
    "keyFile": "../../Dataline.ExtraStandard.snk",
    "compile": "../../GlobalAssemblyInfo.cs",
    "embed": "Schemas/*.xsd"
  },

  "packOptions": {
    "summary": "Unterstützte Krypto-Verfahren für den eXTra-Standard",
    "owners": [ "DATALINE GmbH & Co. KG" ],
    "licenseUrl": "https://github.com/dataline-gmbh/ExtraStandard/blob/master/LICENSE.md",
    "tags": [ "eXTra", "standard", "1.4", "krypto", "verschlüsselung", "pkcs7" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/dataline-gmbh/ExtraStandard"
    },
    "files": {
      "mappings": {
        "/": "../../*.md"
      }
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "Portable.BouncyCastle": "1.8.1.1",
    "Dataline.ExtraStandard": {
      "target": "project"
    }
  },

  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "System.Security.Cryptography.X509Certificates": "4.1.0"
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Security": ""
      }
    }
  }
}
